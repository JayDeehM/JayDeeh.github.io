    <span id='add-portfolio'><%= link_to 'New Port', new_port_path %></span> 


    <% @ports.each do |port| %>
      <div class="column">
        <div class="col-md-4">
          <div class="thumbnail">
            <%= image_tag port.image.url(:medium) %>
            <div class="caption">
              <h1><%= port.title %></h1>
                <span id='update-portfolio'>
                  <%= link_to 'Edit', edit_port_path(port) %> |
                  <%= link_to 'Destroy', port, method: :delete, data: { confirm: 'Are you sure?' } %>
                </span>
            </div>
          </div>
        </div>
      </div>
    <% end %>



  <span id='add-portfolio1'><%= link_to 'New Skill', new_skill_path %></span> 

     <% @skills.each do |skill| %>
      <div id='skillz'>
        <div id='skillimagecontainer'>
          <%= image_tag skill.image.url(:medium), id:'skillimage' %>
        </div>
        <div id="skilldesc">
          <h1 id='sp'><%= skill.title %></h1>
            <span id='update-portfolio1'>
              <%= link_to 'Edit', edit_skill_path(skill) %> |
              <%= link_to 'Destroy', skill, method: :delete, data: { confirm: 'Are you sure?' } %>
            </span>
        </div>
      </div>
    <% end %>


  
